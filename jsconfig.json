// @js-ignore
{
  "compilerOptions": {
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@Components/*": ["src/components/*"],
      "@Views/*": ["src/components/views/*"],
      "@Styles/*": ["styles/*"],
      "@Lib/*": ["src/lib/*"]
    }
  },
  "include": ["./src/**/*", "./pages/**/*"],
  "exclude": ["node_modules"]
}
